<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.22621.0</TargetFramework>
    <RootNamespace>Echo_Orbit</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>EchoOrbitICON.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="defaultAudioImage.jpg" />
    <None Remove="defaultGroup.png" />
    <None Remove="defaultProfile.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="EchoOrbitICON.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SqlServer.TransactSql.ScriptDom" Version="170.23.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="TagLibSharp" Version="2.3.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="defaultAudioImage.jpg" />
    <Resource Include="defaultGroup.png" />
    <Resource Include="defaultProfile.png" />
  </ItemGroup>

</Project>
